{
  "name": "Samsung Article Scraper",
  "description": "Samsung Article Scraper",
  "version": "0.1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "serviceWorker.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["contentScript.js"]
    }
  ],
  "permissions": ["storage", "tabs", "sidePanel", "webNavigation"],
  "host_permissions": ["http://localhost:8000/, https://127.0.0.1:8000"],
  "action": {
    "default_title": "Open VHEY",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "128": "/icons/128x.png"
    }
  },
  "side_panel": {
    "default_path": "sidepanel.html",
    "default_icon": {
      "16": "/icons/16x.png",
      "32": "/icons/32x.png",
      "128": "/icons/128x.png"
    }
  },
  "icons": {
    "16": "/icons/16x.png",
    "32": "/icons/32x.png",
    "128": "/icons/128x.png"
  }
}
